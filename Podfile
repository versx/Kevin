# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'
project 'Jarvis++.xcodeproj'
workspace 'Jarvis'
target 'Jarvis++' do
  # Comment the next line if you don't want to use dynamic frameworks
  #use_frameworks!
  inherit! :search_paths

  # Pods for Jarvis++
  pod 'CocoaHTTPServer', :git => 'https://github.com/robbiehanson/CocoaHTTPServer.git'
  pod 'KIF', :git => 'https://github.com/kif-framework/KIF.git'
end